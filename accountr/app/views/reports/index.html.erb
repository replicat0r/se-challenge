<div class="row dashboard">
	<div class="container">
		<h4 class="header">Expense Overview</h4>
		  <a class="btn-floating btn-large waves-effect waves-light uploadnew red"><i class="material-icons">add</i></a>

	</div>
</div>

<div class="container">
<% if @reports.blank? %>
   <div class="row">
	<%= form_for @report do |f| %>
	<%= f.file_field :file ,class: "inputfile" %>
	<label for="report_file">
		<figure>
			<i class="fa fa-upload"></i>
		</figure>
		<span>Choose a file&hellip;</span></label>
		<div class="col s12">
			<%= f.submit "Upload File", class:"waves-effect waves-light uploadbtn btn" %>
		</div>
		<% end %>
	</div>
	<% else %>
	<div class="row">
	<div class="col s12">
	<table class="responsive-table">
		<thead>
			<tr>
				<% @column_name.each do |name| %>
				<th>
					<%= name.titleize %>
				</th>
				<% end %>
			</tr>
		</thead>
		<tbody>
			<% @reports.each do |report| %>
			<tr>
				<% reject_column_name(report).each do |k,v| %>
				<td>
					<%= v %>
				</td>
				<% end %>
			</tr>
			<% end %>
		</tbody>
	</table>
	</div>
	</div>
	<% end %>

	</div>