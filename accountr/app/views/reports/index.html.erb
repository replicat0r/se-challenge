<div class="row">

<%= form_for @report do |f| %>
    <%= f.file_field :file ,class: "inputfile" %>
    <label for="report_file">
    <figure>
      <i class="fa fa-upload"></i>
    </figure>
     <span>Choose a file&hellip;</span></label>
  <div class="col s12">
  <%= f.submit "Upload File", class:"waves-effect waves-light btn" %>
  </div>
<% end %>

</div>


<table class="responsive-table">
  <thead>
    <tr>
      <% @column_name.each do |name| %>
        <td>
          <%= name.titleize %>
        </td>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <% reject_column_name(report).each do |k,v| %>
          <td>
            <%= v %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>