<div class="row">
  <div class="col s12">
    <table class="responsive-table highlight bordered">
      <thead>
        <tr>
          <% @column_name.each do |name| %>
          <th>
            <%= name.titleize %>
          </th>
          <% end %>
        </tr>
      </thead>
      <tbody>
        <% @reports.each do |report| %>
        <tr>
          <% reject_column_name(report).each do |k,v| %>
          <td>
            <%= v %>
          </td>
          <% end %>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>